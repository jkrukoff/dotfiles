Host github.com
	KexAlgorithms curve25519-sha256@libssh.org,diffie-hellman-group-exchange-sha256,diffie-hellman-group-exchange-sha1,diffie-hellman-group14-sha1
	IdentityFile ~/.ssh/git.id_rsa
	PasswordAuthentication yes
	ChallengeResponseAuthentication yes
	ForwardAgent no
	ForwardX11 no

Host gitlab.bit9.local
	IdentityFile ~/.ssh/git.id_rsa
	ForwardAgent no
	ForwardX11 no

host *.ops.cbdtest.io
	IdentityFile ~/.ssh/confer-test.id
	User jkrukoff
	Compression yes

host mosaic-dev bastion-dev-mosaic-dev-240a2b1fdc421cab.elb.us-east-1.amazonaws.com
	HostName bastion-dev-mosaic-dev-240a2b1fdc421cab.elb.us-east-1.amazonaws.com
	IdentityFile ~/.ssh/mosaic-dev.id_rsa
	User ubuntu
	DynamicForward 8080

host mosaic-sbx bastion-sbx-mosaic-sbx-4ecfd5b02eefc81a.elb.us-east-1.amazonaws.com
	HostName bastion-sbx-mosaic-sbx-4ecfd5b02eefc81a.elb.us-east-1.amazonaws.com
	IdentityFile ~/.ssh/mosaic-sbx.id_rsa
	User ubuntu
	DynamicForward 8080

# Defaults
Host *
	ChallengeResponseAuthentication no
	Ciphers chacha20-poly1305@openssh.com,aes256-gcm@openssh.com,aes128-gcm@openssh.com,aes256-ctr,aes192-ctr,aes128-ctr
	ForwardAgent yes
	ForwardX11 yes
	HostKeyAlgorithms ssh-ed25519-cert-v01@openssh.com,ssh-rsa-cert-v01@openssh.com,ssh-ed25519,ssh-rsa
	KexAlgorithms curve25519-sha256@libssh.org,diffie-hellman-group-exchange-sha256
	MACs hmac-sha2-512-etm@openssh.com,hmac-sha2-256-etm@openssh.com,umac-128-etm@openssh.com,hmac-sha2-512,hmac-sha2-256,umac-128@openssh.com
	PasswordAuthentication no
	Protocol 2
	PubkeyAuthentication yes
	ServerAliveInterval 15
	UseRoaming no
	User jkrukoff
