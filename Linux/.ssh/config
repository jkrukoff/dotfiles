	IdentityFile ~/.ssh/git.id_rsa
	ForwardAgent no
	ForwardX11 no

Host gitlab.bit9.local
	IdentityFile ~/.ssh/git.id_rsa
	ForwardAgent no
	ForwardX11 no

host mosaic-dev bastion-dev-mosaicdevvpc-d30499656278a4ca.elb.us-east-1.amazonaws.com
	HostName bastion-dev-mosaicdevvpc-d30499656278a4ca.elb.us-east-1.amazonaws.com
	IdentityFile ~/.ssh/mosaic-dev.id_rsa
	User ubuntu
	DynamicForward 8080

host mosaic-sbx bastion-sbx-riptidevpc-181e3f22a24c0829.elb.us-east-1.amazonaws.com
	HostName bastion-sbx-riptidevpc-181e3f22a24c0829.elb.us-east-1.amazonaws.com
	IdentityFile ~/.ssh/mosaic-sbx.id_rsa
	User ubuntu
	DynamicForward 8080

